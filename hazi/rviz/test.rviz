# your_rviz_config.rviz
config: &rviz_config
  Visualization Manager:
    Global Options:
      Fixed Frame: "map"  # Adjust the fixed frame based on your setup

  PanelDockWidgetArea: 1

  Panels:
    - class: "rviz/map"
      config: {}
    - class: "rviz/PointCloud"
      config:
        Name: "/cloud"
        Topic: "/cloud"
    - class: "rviz/MarkerArray"
      config:
        Name: "/cone_markers"
        Topic: "/cone_markers"
